package Assessment6;

class MethodOROL {

	public void MethodOL() 
	{
		System.out.println("MethodOL");
	}
	int MethodOL(int i,int j)
	{
		return i + j;
	}
	
	
}

public class MethodOR extends MethodOROL
{	
	
	
	int MethodOL(int i,int j)
	{
		System.out.println( super.MethodOL(1,2));	
		 return i*j;
	}
	public static void main(String[] args)
	{
		MethodOR M1 = new MethodOR();
		M1.MethodOL();
		System.out.println(M1.MethodOL(1,3));
		
	}
	
}