package Assessment6;

public class MethodOLandOR {

	public void MethodOL() 
	{
		System.out.println("MethodOL");
	}
	int MethodOL(int i,int j)
	{
		return i + j;
	}
	public static void main(String[] args)
	{
		MethodOLandOR M1 = new MethodOLandOR();
		System.out.println(M1.MethodOL(1,3));
		System.out.println(M1.MethodOL());
		
	}
	
}

class MethodOR extends MethodOLandOR
{	
	
	
	int MethodOL(int i,int j)
	{
		 super.MethodOL(1,2);	
		 return i*j;
	}
	
}