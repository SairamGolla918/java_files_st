package myPack;

class creditCard implements Payment 
{
	
	int cardNumber;
	int expirationDate;
	int CVV;
	public creditCard() {}
	
	public creditCard(int cardNumber,int expirationDate,int CVV) 
	{
		this.cardNumber = cardNumber;
		this.expirationDate = expirationDate;
		this.CVV = CVV;
		
	}
	public void processPayment(double amount)
	{
		System.out.println("credit card processing");
	}

}

class debitCard implements Payment
{
	int cardNumber;
	int expirationDate;
	int CVV;
	public debitCard() {}
	public debitCard(int cardNumber,int expirationDate,int CVV)
	{	
		this.cardNumber = cardNumber;
		this.expirationDate = expirationDate;
		this.CVV = CVV;
		
	}
	public void processPayment(double amount)
	{
		System.out.println("Select the Transaction");
	}
}

class cashOnDelivery implements Payment
{
	double amount;
	public cashOnDelivery() {}
	public cashOnDelivery(double amount) {
		this.amount = amount;
	}
	public void processPayment(double amount)
	{
		System.out.println("cash on delivery process");
	}
}

class paymentProcessor implements Payment
{
	void makePayment(Payment payment,double amount)
	{
		payment.processPayment(amount);
	}
	void makePayment(int cardNumber,int expirationDate,int CVV,double amount)
	{
		creditCard cdCard = new creditCard(7666,2554,661);
		
	}
	
	public static void main(String[] args) {
		Payment paymentObj = new paymentProcessor();
		
		debitCard dbCard = new debitCard(7666,2554,661);
		cashOnDelivery cod = new cashOnDelivery(661.00);
	}
}
