<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.product.beans.Product" table="Product" lazy="false">
		<id name="productId" type="int">
		<generator class="assigned"></generator>
		</id>
		<property name="productName" column="Product_Name"></property>
		<property name="productPrice" column="Product_Price"></property>
		<property name="discount" column="Discount"></property>
		
	</class>
</hibernate-mapping>